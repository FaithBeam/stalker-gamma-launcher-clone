{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "anomaly-install": {
      "commandName": "Project",
      "commandLineArgs": "anomaly-install --verbose"
    },
    "check-anomaly": {
      "commandName": "Project",
      "commandLineArgs": "check-anomaly"
    },
    "full-install": {
      "commandName": "Project",
      "commandLineArgs": "full-install --verbose"
    },
    "full-install-update": {
      "commandName": "Project",
      "commandLineArgs": "full-install --verbose --skip-extract-on-hash-match --skip-github-downloads"
    },
    "debug-hash-install-blake3": {
      "commandName": "Project",
      "commandLineArgs": "debug hash-install --hash-type blake3"
    },
    "debug-hash-install-sha256": {
      "commandName": "Project",
      "commandLineArgs": "debug hash-install --hash-type sha256"
    },
    "profiles-list": {
      "commandName": "Project",
      "commandLineArgs": "profiles list"
    },
    "config-create": {
      "commandName": "Project",
      "commandLineArgs": "config create --anomaly /Volumes/NewVolume/gamma/anomaly --gamma /Volumes/NewVolume/gamma/gamma --cache /Volumes/NewVolume/gamma/cache --download-threads 4"
    },
    "config-create-ccon": {
      "commandName": "Project",
      "commandLineArgs": "config create --name gamma-with-ccon --anomaly /Volumes/NewVolume/gamma/anomaly --gamma /Volumes/NewVolume/gamma/gamma --cache /Volumes/NewVolume/gamma/cache --download-threads 4 --mo2-profile gamma-with-ccon --mod-pack-maker-url https://raw.githubusercontent.com/FaithBeam/stalker-gamma-ccon-modlist/refs/heads/master/autumn/modpack_maker_list.txt --mod-list-url https://raw.githubusercontent.com/FaithBeam/stalker-gamma-ccon-modlist/refs/heads/master/autumn/modlist.txt"
    },
    "config-use-gamma": {
      "commandName": "Project",
      "commandLineArgs": "config use gamma"
    },
    "config-use-ccon": {
      "commandName": "Project",
      "commandLineArgs": "config use gamma"
    },
    "config-list": {
      "commandName": "Project",
      "commandLineArgs": "config list"
    },
    "update-list": {
      "commandName": "Project",
      "commandLineArgs": "update list"
    },
    "upgrade-apply": {
      "commandName": "Project",
      "commandLineArgs": "update apply"
    }
  }
}
